{
  "name": "isomorphic-preact",
  "version": "1.0.0",
  "description": "isomorphic architecture preact example",
  "author": "malbernaz <albernazmiguel@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:dev:client": "webpack --env dev:client",
    "build:dev:server": "webpack --env dev:server",
    "build:prod:client": "webpack --env prod:client",
    "build:prod:server": "webpack --env prod:server",
    "build:dev": "yarn run build:dev:client && yarn run build:dev:server",
    "build:prod": "yarn run build:prod:client && yarn run build:prod:server",
    "nodemon": "nodemon --watch dist --exec 'node --inspect' dist",
    "start": "yarn run build:dev && concurrently --raw --kill-others 'yarn run watch:server' 'yarn run nodemon' 'yarn run watch:client'",
    "watch:client": "webpack-dev-server --env dev:client --inline --hot --progress",
    "watch:server": "webpack --env dev:server --watch"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.8.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^3.0.0",
    "isomorphic-style-loader": "^1.1.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "postcss-css-variables": "^0.6.0",
    "postcss-import": "^8.2.0",
    "postcss-loader": "^1.1.1",
    "rucksack-css": "^0.8.6",
    "stylelint": "^7.5.0",
    "stylelint-config-css-modules": "^0.1.0",
    "stylelint-config-standard": "^14.0.0",
    "stylelint-webpack-plugin": "^0.4.0",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.11"
  },
  "dependencies": {
    "express": "^4.14.0",
    "history": "^4.4.0",
    "normalize.css": "^5.0.0",
    "preact": "^6.4.0",
    "preact-render-to-string": "^3.2.1",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.11.1",
    "universal-router": "^2.0.0"
  }
}
